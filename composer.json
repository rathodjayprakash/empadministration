{
    "name": "ms/empadministration",
    "type": "typo3-cms-extension",
    "description": "An employee administration for the personnel department of a company is desired.",
    "authors": [
        {
            "name": "Jayprakash",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^9.5"
    },
    "autoload": {
        "psr-4": {
